{
    "name": "Jina URL Redirect",
    "description": "Opens current URL prefixed with r.jina.ai in a new tab",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_title": "Open in Jina"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }